#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/moe/Documents/code/javascript/robotist/node_modules/.pnpm/@web+dev-server@0.0.25/node_modules/@web/dev-server/dist/node_modules:/Users/moe/Documents/code/javascript/robotist/node_modules/.pnpm/@web+dev-server@0.0.25/node_modules/@web/dev-server/node_modules:/Users/moe/Documents/code/javascript/robotist/node_modules/.pnpm/@web+dev-server@0.0.25/node_modules/@web/node_modules:/Users/moe/Documents/code/javascript/robotist/node_modules/.pnpm/@web+dev-server@0.0.25/node_modules:/Users/moe/Documents/code/javascript/robotist/node_modules/.pnpm/node_modules:/Users/moe/Documents/code/javascript/robotist/node_modules:/Users/moe/Documents/code/javascript/node_modules:/Users/moe/Documents/code/node_modules:/Users/moe/Documents/node_modules:/Users/moe/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/moe/Documents/code/javascript/robotist/node_modules/.pnpm/@web+dev-server@0.0.25/node_modules/@web/dev-server/dist/node_modules:/Users/moe/Documents/code/javascript/robotist/node_modules/.pnpm/@web+dev-server@0.0.25/node_modules/@web/dev-server/node_modules:/Users/moe/Documents/code/javascript/robotist/node_modules/.pnpm/@web+dev-server@0.0.25/node_modules/@web/node_modules:/Users/moe/Documents/code/javascript/robotist/node_modules/.pnpm/@web+dev-server@0.0.25/node_modules:/Users/moe/Documents/code/javascript/robotist/node_modules/.pnpm/node_modules:/Users/moe/Documents/code/javascript/robotist/node_modules:/Users/moe/Documents/code/javascript/node_modules:/Users/moe/Documents/code/node_modules:/Users/moe/Documents/node_modules:/Users/moe/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin.js" "$@"
else
  exec node  "$basedir/../../dist/bin.js" "$@"
fi
